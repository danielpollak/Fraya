<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fraya"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <!-- android:icon="@drawable/ic_launcher" -->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.fraya.Hekko"
            android:label="@string/title_activity_hekko" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.fraya.Morse"
            android:label="@string/title_activity_morse" >
        </activity>
        <activity
            android:name="com.example.fraya.GameOfLife"
            android:label="@string/title_activity_game_of_life" >
        </activity>
    </application>

</manifest>